    <scope>test</scope>
</dependency>
  </dependencies><!-- -->
  <build>
  <plugins>
  <plugin>
    <groupId>org.apache.maven.plugins</groupId>
    <artifactId>maven-compiler-plugin</artifactId>
    <version>3.8.1</version>
    <configuration>
        <source>23</source>  <!-- Or your correct Java version -->
        <target>23</target>
    </configuration>
</plugin>
</plugins>
  </build>
</project>